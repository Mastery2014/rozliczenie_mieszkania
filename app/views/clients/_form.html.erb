<%= simple_form_for @client do |f| %>
<%= f.input :name, label: 'Imie' %>
<%= f.input :surname, label: 'Nazwisko' %>
<%= f.input :phone, label: 'Telefon' %>
<%= f.input :email, label: 'Email' %>
<%= f.input :rent_start, label: 'Poczatek' %>
<%= f.input :rent_end, label: 'Koniec' %>
<%= f.button :submit %>
<% end %>
